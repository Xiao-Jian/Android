public class test
{
	public static void main(String[] args) 
	{
		System.out.println("t        (I) ");
		int t=0;
		int z=5;
		int w=10;
		int Y=6000;
		double R1,R2,D;
		double G=10000.00;
		double I=1000.00;
		double [] DATA = new double[1001];
		D=Y-I;
		R1=D/z;
		R2=G/w;
		System.out.println(t+"        "+I);
		while(t<1000)
		{
			t=t+1;
			I=I+R2;
			G=G+R1-R2;
			D=Y-I;
			R1=D/z;
			R2=G/w;
			//System.out.println(t+"        "+I);
			DATA[t]=I;
		}
	}
}